{
  "name": "react-mentoring",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"yarn run start:be\" \"yarn run start:fe\"",
    "build-dev": "rimraf ./dist && webpack --env dev",
    "build-prod": "rimraf ./dist && webpack --env prod",
    "start:be": "json-server --watch db.json",
    "start:fe": "yarn run build-dev && webpack-dev-server --open --env dev",
    "test": "jest"
  },
